    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-react": "7.24.0",
    "npm-run-all": "4.1.5",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "typescript": ">=3.5"
  },
  "private": true,
